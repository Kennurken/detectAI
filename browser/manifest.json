{
  "manifest_version": 3,
  "name": "AI Cyber-Detective",
  "version": "1.2",
  "description": "Экранды және мәтінді AI арқылы қауіпсіздікке талдау",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://detect-ai-silk.vercel.app/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}